  },
  "devDependencies": {
    "@angular/compiler": "^19.0.6",
    "@eslint/js": "^9.18.0",
    "@types/braces": "^3.0.5",
    "@types/gettext-parser": "^4.0.4",
    "@types/node": "^18.19.70",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.19.1",
